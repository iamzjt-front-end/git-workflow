{
	"name": "git-workflow",
	"version": "0.0.1",
	"description": "个人常用的 Git 工作流工具，快速创建规范的开发分支和管理 Tag",
	"type": "module",
	"bin": {
		"git-flow": "./dist/index.js",
		"gw": "./dist/index.js"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsx src/index.ts",
		"changelog": "changelogen --output CHANGELOG.md",
		"release": "changelogen --release && npm run build",
		"prepublishOnly": "npm run build",
		"prepare": "husky"
	},
	"keywords": [
		"git",
		"workflow",
		"branch",
		"tag",
		"cli"
	],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": ""
	},
	"dependencies": {
		"@inquirer/prompts": "^7.0.0",
		"cac": "^6.7.14",
		"ora": "^9.0.0"
	},
	"devDependencies": {
		"@types/node": "^25.0.3",
		"changelogen": "^0.6.2",
		"husky": "^9.1.7",
		"tsup": "^8.5.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	}
}
